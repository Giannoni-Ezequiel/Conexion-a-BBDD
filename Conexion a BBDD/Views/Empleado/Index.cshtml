@using System.Linq;
@model List<Empleado>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1> Los clientes son:</h1>

@foreach (var item in Model)
{
    <th>@Html.DisplayFor(m => item.id_empleado)</th>
    <p>---------------</p>
    <th>@Html.DisplayFor(m => item.emple_nombre)</th>
    <p>---------------</p>

    <th>@Html.DisplayFor(m => item.emple_apellido)</th>
    <p>---------------</p>
    <th>@Html.DisplayFor(m => item.emple_id_supervisor)</th>
    <p>---------------</p>
    <th>@Html.DisplayFor(m => m.Where(s => s.emple_id_usuario == 1))</th>
    <p>---------------</p>




}